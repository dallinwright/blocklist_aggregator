  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.1",
    "@types/chai-as-promised": "^7.1.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "husky": "^4.2.5",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2"
  }
