{
  "name": "blocklist-aggregator",
  "version": "1.0.0",
  "description": "Take a list of IP's and create a custom blocklist",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.59",
    "aws-lambda": "^1.0.6",
